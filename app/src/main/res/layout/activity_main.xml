<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <ImageButton
            android:layout_width="125dp"
            android:layout_height="125dp" app:srcCompat="@android:color/holo_blue_bright"
            android:id="@+id/imageButton"
            app:layout_constraintEnd_toStartOf="@+id/imageButton2"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="89dp"
            android:layout_marginStart="89dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="190dp"/>
    <ImageButton
            android:layout_width="125dp"
            android:layout_height="125dp" app:srcCompat="@android:color/holo_green_dark"
            android:id="@+id/imageButton2"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="190dp" android:layout_marginEnd="64dp"
            android:layout_marginRight="64dp" app:layout_constraintEnd_toEndOf="parent"/>
    <ImageButton
            android:layout_width="125dp"
            android:layout_height="125dp" app:srcCompat="@android:color/holo_blue_bright"
            android:id="@+id/imageButton3"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintEnd_toStartOf="@+id/imageButton4" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageButton" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="89dp" android:layout_marginStart="89dp"/>
    <ImageButton
            android:layout_width="125dp"
            android:layout_height="125dp" app:srcCompat="@android:color/holo_green_dark"
            android:id="@+id/imageButton4" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageButton2" android:layout_marginEnd="64dp"
            android:layout_marginRight="64dp" app:layout_constraintEnd_toEndOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>